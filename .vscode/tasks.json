{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: aarch64-linux-gnu-gcc 활성 파일 빌드",
            "command": "/usr/bin/aarch64-linux-gnu-gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "1_Background/Background.c",
                "2_LinkedList/LinkedList.c",
                "common/common.c",
                "main.c",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "디버거에서 생성된 작업입니다."
        }
    ],
    "version": "2.0.0"
}